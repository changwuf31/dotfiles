# This tmux statusbar config was created by tmuxline.vim
# on Mon, 24 Jun 2019

set -g status-justify "left"
set -g status "on"
set -g status-left-style "none"
set -g message-command-style "fg=#68607d,bg=#eae9dd"
set -g status-right-style "none"
set -g pane-active-border-style "fg=#68607d"
set -g status-style "none,bg=#f8f2e5"
set -g message-style "fg=#68607d,bg=#eae9dd"
set -g pane-border-style "fg=#eae9dd"
set -g status-right-length "100"
set -g status-left-length "100"
setw -g window-status-activity-style "none"
setw -g window-status-separator ""
setw -g window-status-style "none,fg=#68607d,bg=#f8f2e5"
set -g status-left "#[fg=#f8f2e5,bg=#68607d,bold] #S #[fg=#68607d,bg=#f8f2e5,nobold,nounderscore,noitalics]"
set -g status-right "#[fg=#eae9dd,bg=#f8f2e5,nobold,nounderscore,noitalics]#[fg=#68607d,bg=#eae9dd] %Y-%m-%d  %H:%M #[fg=#68607d,bg=#eae9dd,nobold,nounderscore,noitalics]#[fg=#f8f2e5,bg=#68607d] #h "
setw -g window-status-format "#[fg=#68607d,bg=#f8f2e5] #I #[fg=#68607d,bg=#f8f2e5] #W "
setw -g window-status-current-format "#[fg=#f8f2e5,bg=#eae9dd,nobold,nounderscore,noitalics]#[fg=#68607d,bg=#eae9dd] #I #[fg=#68607d,bg=#eae9dd] #W #[fg=#eae9dd,bg=#f8f2e5,nobold,nounderscore,noitalics]"
